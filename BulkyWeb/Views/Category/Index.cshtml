@model List<Category>
<h1>Category Page</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Category Name</th>
            <th>Display Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model.OrderBy(u=>u.DisplayOrder))
        {
            <tr>
                <td>@i.Name</td>
                <td>@i.DisplayOrder</td>
            </tr>
        }
    </tbody>
    
</table>